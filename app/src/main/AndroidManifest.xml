<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zenserapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ZenserApp">
        <activity android:name=".ui.categories.DealsPage"></activity>
        <activity android:name=".ui.categories.WomenFashionHome" />
        <activity android:name=".ui.categories.MenFashionHome" />
        <activity android:name=".ui.categories.EducationHome" />
        <activity android:name=".ui.categories.VoucherHome" />
        <activity android:name=".ui.categories.ComputerAndTechHome" />
        <activity android:name=".ui.categories.ServicesHome" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>